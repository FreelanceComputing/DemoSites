Options +SymLinksIfOwnerMatch -MultiViews
<IfModule mod_rewrite.c>
RewriteEngine On

#If your website is installed in a subfolder, change the line below to reflect the path to the subfolder.
#e.g. for http://www.example.com/subdomain1/subdomain2/ make it RewriteBase /subdomain1/subdomain2
RewriteBase /

#If you wish to use a custom 404 page, place a file named 404.php in your website's root and uncomment the line below.
#If your website is installed in a subfolder, change the line below to reflect the path to the subfolder.
#e.g. for http://www.example.com/subdomain1/subdomain2/ make it ErrorDocument 404 /subdomain1/subdomain2/404.php
#ErrorDocument 404 /404.php

#If your site begins with 'www', uncomment the following two lines
#RewriteCond %{HTTP_HOST} !^www\.
#RewriteRule ^(.*)$ http://www.%{HTTP_HOST}/$1 [R=301,L]


#DO NOT EDIT BELOW THIS

RewriteRule ^gallery/index.php$ "gallery/" [R=301,L,QSA]
RewriteRule ^faq/index.php$ "faq/" [R=301,L,QSA]
RewriteRule ^contact/index.php$ "contact/" [R=301,L,QSA]
RewriteRule ^index.php$ "" [R=301,L,QSA]

RewriteCond %{REQUEST_FILENAME} -d [OR]
RewriteCond %{REQUEST_FILENAME} -f
RewriteRule . - [L]

#gallery/pictures.php
RewriteRule ^gallery/pictures$ "$0/" [R=301,L,QSA]
RewriteRule ^gallery/pictures/$ gallery/pictures.php [L,QSA]
RewriteRule ^gallery/pictures/.*?([^\.\/]*)\.html$ gallery/pictures.php?pname=$1 [L,QSA]
RewriteRule ^gallery/pictures/([1-2]\d{3})/(?:(0[1-9]|1[0-2])/(?:(0[1-9]|1[0-9]|2[0-9]|3[0-1])/)?)?$ gallery/pictures.php?d=$1$2$3 [L,QSA]
RewriteRule ^gallery/pictures/[^\.]*?([^/\.]*)/$ gallery/pictures.php?fname=$1 [L,QSA]
RewriteRule ^gallery/pictures/[^\.]*?([^/\.]*)$ "$0/" [R=301,L,QSA]

#gallery/index.php
RewriteRule ^gallery/.*?([^\.\/]*)\.html$ gallery/?pname=$1 [L,QSA]
RewriteRule ^gallery/([1-2]\d{3})/(?:(0[1-9]|1[0-2])/(?:(0[1-9]|1[0-9]|2[0-9]|3[0-1])/)?)?$ gallery/?d=$1$2$3 [L,QSA]
RewriteRule ^gallery/[^\.]*?([^/\.]*)/$ gallery/?fname=$1 [L,QSA]
RewriteRule ^gallery/[^\.]*?([^/\.]*)$ "$0/" [R=301,L,QSA]

#faq/index.php
RewriteRule ^faq/.*?([^\.\/]*)\.html$ faq/?pname=$1 [L,QSA]
RewriteRule ^faq/([1-2]\d{3})/(?:(0[1-9]|1[0-2])/(?:(0[1-9]|1[0-9]|2[0-9]|3[0-1])/)?)?$ faq/?d=$1$2$3 [L,QSA]
RewriteRule ^faq/[^\.]*?([^/\.]*)/$ faq/?fname=$1 [L,QSA]
RewriteRule ^faq/[^\.]*?([^/\.]*)$ "$0/" [R=301,L,QSA]

#contact/index.php
RewriteRule ^contact/.*?([^\.\/]*)\.html$ contact/?pname=$1 [L,QSA]
RewriteRule ^contact/([1-2]\d{3})/(?:(0[1-9]|1[0-2])/(?:(0[1-9]|1[0-9]|2[0-9]|3[0-1])/)?)?$ contact/?d=$1$2$3 [L,QSA]
RewriteRule ^contact/[^\.]*?([^/\.]*)/$ contact/?fname=$1 [L,QSA]
RewriteRule ^contact/[^\.]*?([^/\.]*)$ "$0/" [R=301,L,QSA]

#events.php
RewriteRule ^events$ "$0/" [R=301,L,QSA]
RewriteRule ^events/$ events.php [L,QSA]
RewriteRule ^events/.*?([^\.\/]*)\.html$ events.php?pname=$1 [L,QSA]
RewriteRule ^events/([1-2]\d{3})/(?:(0[1-9]|1[0-2])/(?:(0[1-9]|1[0-9]|2[0-9]|3[0-1])/)?)?$ events.php?d=$1$2$3 [L,QSA]
RewriteRule ^events/[^\.]*?([^/\.]*)/$ events.php?fname=$1 [L,QSA]
RewriteRule ^events/[^\.]*?([^/\.]*)$ "$0/" [R=301,L,QSA]

#index.php
RewriteRule ^.*?([^\.\/]*)\.html$ ?pname=$1 [L,QSA]
RewriteRule ^([1-2]\d{3})/(?:(0[1-9]|1[0-2])/(?:(0[1-9]|1[0-9]|2[0-9]|3[0-1])/)?)?$ ?d=$1$2$3 [L,QSA]
RewriteRule ^[^\.]*?([^/\.]*)/$ ?fname=$1 [L,QSA]
RewriteRule ^\w[^\.]*?([^/\.]*)$ "$0/" [R=301,L,QSA]
</IfModule>